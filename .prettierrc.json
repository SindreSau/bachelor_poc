{
    "plugins": [
        "prettier-plugin-tailwindcss"
    ],
    "singleQuote": true,
    "jsxSingleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "endOfLine": "lf",
    "bracketSameLine": false
}